{
  "name": "libraries",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "commonjs",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "bs-dependencies": [
    "@dzakh/rescript-stdlib",
    "rescript-envsafe",
    "rescript-struct",
    "@glennsl/rescript-fetch"
  ],
  "bsc-flags": ["-open Stdlib", "-open ReScriptStruct"],
  "gentypeconfig": {
    "language": "typescript",
    "shims": { "Js": "Js" },
    "generatedFileExtension": ".gen.ts",
    "module": "commonjs",
    "debug": {
      "all": false,
      "basic": false
    }
  }
}
